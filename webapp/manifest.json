{
	"_version": "1.58.0",
	"sap.app": {
		"id": "ui5.walkthrough",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			],
			"fallbackLocale": ""
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"invoiceRemote": {
				"uri": "V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"dependencies": {
			"minUI5Version": "1.120.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.uxap": {},
				"sap.f": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ui5.walkthrough.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			},
			"invoice": {
				"dataSource": "invoiceRemote"
			}
		},
		"rootView": {
			"viewName": "ui5.walkthrough.view.App",
			"type": "XML",
			"id": "app"
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "ui5.walkthrough.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				
				{
					"pattern": "",
          			"name": "home",
          			"target": "home"
				},
				{
					"pattern": "otelprofile",
					"name": "otelprofile",
					"target": "OtelProfile"
				  },
				  {
					"pattern": "otelprofile/{productId}",
					"name": "productDetails",
					"target": "OtelProfile"
				  },
				  {
					"pattern": "livrare",
					"name": "ModalitatiLivrare",
					"target": "ModalitatiLivrare"
				  }
				
			],
			"targets": {
				"home": {
          "viewName": "Website_firstpage",   
          "viewId": "home",
          "viewLevel": 1
        },
		"OtelProfile": {
 			 "viewName": "OtelProfile",
  				"viewId": "otelprofile",
  			"viewLevel": 2
		},
		"ModalitatiLivrare": {
			"viewName": "ModalitatiLivrare"
		 }	
	}

		}
	}
}
